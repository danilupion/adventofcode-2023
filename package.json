{
    "name": "adventofcode-2023",
    "version": "1.0.0",
    "author": "danilupion <danilupion@gmail.com>",
    "type": "module",
    "license": "MIT",
    "prviate": true,
    "scripts": {
        "prettify": "prettier --write  \"./src/**/*.{js,json}\"",
        "lint": "eslint -c .eslintrc.cjs 'src/**/*.js'",
        "lint:fix": "eslint -c .eslintrc.cjs 'src/**/*.js' --fix",
        "prepare": "husky install",
        "1-1": "node src/challenges/1/part1/solution.js",
        "1-2": "node src/challenges/1/part2/solution.js",
        "2-1": "node src/challenges/2/part1/solution.js",
        "2-2": "node src/challenges/2/part2/solution.js",
        "3-1": "node src/challenges/3/part1/solution.js",
        "3-2": "node src/challenges/3/part2/solution.js",
        "4-1": "node src/challenges/4/part1/solution.js",
        "4-2": "node src/challenges/4/part2/solution.js",
        "5-1": "node src/challenges/5/part1/solution.js",
        "5-2": "node src/challenges/5/part2/solution.js",
        "6-1": "node src/challenges/6/part1/solution.js",
        "6-2": "node src/challenges/6/part2/solution.js",
        "7-1": "node src/challenges/7/part1/solution.js",
        "7-2": "node src/challenges/7/part2/solution.js"
    },
    "devDependencies": {
        "eslint": "^8.55.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^3.1.0"
    },
    "lint-staged": {
        "src/**/*.{js}": [
            "yarn prettify",
            "yarn lint:fix --max-warnings=0"
        ]
    }
}
